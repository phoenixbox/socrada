<div id="twitter-data">
  <div id="twitter-friends">
    <h2>Twitter Friends</h2>
    <% @twitter_friends.each do |friend|%>
    <ul>
      <li>
        <%= p friend %>
      </li>
    </ul>
    <%end%>
  </div> <!-- end of #twitter-friends-->
  <br>
  <div id="twitter-followers">
    <h2>Twitter Followers</h2>
    <% @twitter_followers.each do |follower|%>
    <ul>
      <li>
        <%= p follower %>
      </li>
    </ul>
    <%end%>
  </div> <!-- end of #twitter-followers-->
  <br>
  <div id="twitter-mutual">
    <h2>Twitter Mutual</h2>
    <% @twitter_mutual.each do |mutual|%>
    <ul>
      <li>
        <%= p mutual %>
      </li>
    </ul>
    <%end%>
  </div> <!-- end of #twitter-mutual-->
</div>